
// **************************** Service Call Dropdown Menu JQuery Script ****************************

$('#scQueue').on('change', function(){
 console.log($('#scQueue').val());
  $('#scQueueResult').html('');

  if($('#scQueue').val()=='Add On'){
    $('#scQueueResult').append('<option value="">-- Select Add-On --</option>');
    $('#scQueueResult').append('<option value="Add On | Mesh Unit">Add On | Mesh Unit</option>');
    $('#scQueueResult').append('<option value="Add On | P2P AP">Add On | P2P AP</option>');
    $('#scQueueResult').append('<option value="Add On | P2P Client">Add On | P2P Client</option>');
    $('#scQueueResult').append('<option value="Add On | Router">Add On | Router</option>');
    $('#scQueueResult').append('<option value="Add On | UniFi">Add On | UniFi</option>');
    $('#scQueueResult').append('<option value="Add On | VoIP">Add On | VoIP</option>');
  }

  if($('#scQueue').val()=='Equipment Removal'){
    $('#scQueueResult').append('<option value="Remove Equipment">Remove Equipment</option>');
  }

  if($('#scQueue').val()=='Installation'){
    $('#scQueueResult').append('<option value="">-- Select Installation --</option>');
    $('#scQueueResult').append('<option value="Install | Bucket Truck">Install | Bucket Truck</option>');
    $('#scQueueResult').append('<option value="Install | Business">Install | Business</option>');
    $('#scQueueResult').append('<option value="Install | CAF">Install | CAF</option>');
    $('#scQueueResult').append('<option value="Install | Commercial">Install | Commercial</option>');
    $('#scQueueResult').append('<option value="Install | Customer Move">Install | Customer Move</option>');
    $('#scQueueResult').append('<option value="Install | FTTH">Install | FTTH</option>');
    $('#scQueueResult').append('<option value="Install | Mast">Install | Mast</option>');
    $('#scQueueResult').append('<option value="Install | Residential">Install | Residential</option>');
    $('#scQueueResult').append('<option value="Install | Same-Day">Install | Same-Day</option>');
    $('#scQueueResult').append('<option value="Install | Tower">Install | Tower</option>');
    $('#scQueueResult').append('<option value="Install | Tower Host">Install | Tower Host</option>');
    $('#scQueueResult').append('<option value="Install | Transfer">Install | Transfer</option>');
  }

  if($('#scQueue').val()=='Modification'){
    $('#scQueueResult').append('<option value="">-- Select Modification --</option>');
    $('#scQueueResult').append('<option value="Mod | Cable Drop">Mod | Cable Drop</option>');
    $('#scQueueResult').append('<option value="Mod | Grounding">Mod | Grounding</option>');
    $('#scQueueResult').append('<option value="Mod | Maintenance">Mod | Maintenance</option>');
    $('#scQueueResult').append('<option value="Mod | Mount">Mod | Mount</option>');
    $('#scQueueResult').append('<option value="Mod | Move Equipment">Mod | Move Equipment</option>');
    $('#scQueueResult').append('<option value="Mod | Replace All">Mod | Replace All</option>');
    $('#scQueueResult').append('<option value="Mod | Repoint">Mod | Repoint</option>');
    $('#scQueueResult').append('<option value="Mod | Second Tower Climber">Mod | Second Tower Climber</option>');
    $('#scQueueResult').append('<option value="Mod | Site Survey">Mod | Site Survey</option>');
    $('#scQueueResult').append('<option value="Mod | Upgrade">Mod | Upgrade</option>');
   }

   if($('#scQueue').val()=='Service Call'){
    $('#scQueueResult').append('<option value="">-- Select Service Call --</option>');
    $('#scQueueResult').append('<option value="Service Call | No Connection">Service Call | No Connection</option>');
    $('#scQueueResult').append('<option value="Service Call | Service Degradation">Service Call | Service Degradation</option>');
    $('#scQueueResult').append('<option value="Service Call | Router">Service Call | Router</option>');
    $('#scQueueResult').append('<option value="Service Call | Mesh Unit">Service Call | Mesh Unit</option>');
    $('#scQueueResult').append('<option value="Service Call | Replace Equipment">Service Call | Replace Equipment</option>');
    $('#scQueueResult').append('<option value="Service Call | Ethernet">Service Call | Ethernet</option>');
    $('#scQueueResult').append('<option value="Service Call | Tower Service Call">Service Call | Tower Service Call</option>');
    $('#scQueueResult').append('<option value="Service Call | UniFi">Service Call | UniFi</option>');
    $('#scQueueResult').append('<option value="Service Call | VoIP">Service Call | VoIP</option>');
    $('#scQueueResult').append('<option value="Service Call | Bucket Truck">Service Call | Bucket Truck</option>');
   }

});

